{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "moduleResolution": "Node",
        "lib": [
            "ES5",
            "DOM",
            "ES2015.Collection",
            "ES2015.Promise",
            "ES2015.Core",
        ],
        "allowJs": true,
        "experimentalDecorators": true,
    }
}
